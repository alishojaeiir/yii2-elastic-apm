{
    "name": "ivoglent/yii2-elastic-apm",
    "description": "Elastic Agent for Yii2 Framework",
    "type": "yii2-extension",
    "version": "1.0.4",
    "require": {
        "php": ">=7.1.0",
        "yiisoft/yii2": "~2.0.14",
        "ivoglent/elastic-apm-agent-v2": "1.0.1"
    },
    "autoload": {
        "psr-4": {
            "ivoglent\\yii2\\apm\\": "./src"
        }
    },
    "license": "BSD-3-Clause",
    "extra": {
        "bootstrap": "ivoglent\\yii2\\apm\\Module"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "authors": [
        {
            "name": "Ivoglent Nguyen",
            "email": "ivoglent@gmail.com"
        }
    ],
    "prefer-stable": true
}
