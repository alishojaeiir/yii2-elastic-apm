{
    "name": "alishojaeiir/yii2-elastic-apm",
    "description": "Elastic Agent for Yii2 Framework",
    "type": "module",
    "require": {
        "php": ">=7.1.0",
        "yiisoft/yii2": "~2.0.14",
        "ivoglent/elastic-apm-agent-v2": "1.0.1"
    },
    "autoload": {
        "psr-4": {
            "ivoglent\\yii2\\apm\\": "./src"
        }
    },
    "license": "BSD-3-Clause",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "authors": [
        {
            "name": "Ali Shojaei",
            "email": "alishojaei34@gmail.com"
        }
    ],
    "prefer-stable": true
}
